<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.springgmvc.workshop.orangecat.photo.dao.PhotoDao">

    <select id="getPhoto" resultType="com.example.springgmvc.workshop.orangecat.photo.bo.Photo" parameterType="Long">
        select * from photo where photoId=#{photoId}
    </select>
</mapper>